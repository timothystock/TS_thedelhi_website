msgid "Saturday"
msgstr "Samstag"

#: opening-hours.php:2203
msgid "Friday"
msgstr "Freitag"

#: opening-hours.php:2202
msgid "Thursday"
msgstr "Donnerstag"

#: opening-hours.php:2201
msgid "Wednesday"
msgstr "Mittwoch"

#: opening-hours.php:2200
